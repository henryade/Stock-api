{
  "name": "stock-api",
  "version": "1.0.0",
  "description": "Stock API",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "node dist/index.js",
    "start:dev": "nodemon --exec babel-node index.js",
    "db:migrate": "sequelize db:migrate",
    "db:migrate-undo": "sequelize db:migrate:undo:all",
    "db:rollback": "npm run db:migrate-undo && npm run db:migrate && npm run db:seed",
    "db:seed": "sequelize db:seed:all",
    "build": "babel . --out-dir dist && npm run copy:docs"
  },
  "repository": {
    "type": "git",
    "url": "henryade"
  },
  "author": "Adeola Adeyemo",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.19.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "path": "^0.12.7",
    "pg": "^7.12.1",
    "pg-hstore": "^2.3.3",
    "sequelize": "^5.21.2"
  },
  "devDependencies": {
    "@babel/core": "^7.6.4",
    "@babel/node": "^7.6.3",
    "@babel/preset-env": "^7.6.3",
    "nodemon": "^1.19.4",
    "sequelize-cli": "^5.5.1"
  }
}
